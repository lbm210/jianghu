.banner{width:100%;height:300rpx;background:url("data:image/png;base64,") no-repeat;background-size:cover;}
.banner text{color:#fff;font-size:24rpx;line-height: 24rpx;}
.banner text:nth-child(1){margin:45rpx 0 18rpx;}
.banner text:nth-child(2){font-size:32rpx;line-height: 32rpx;margin-bottom:40rpx;font-weight:bold;}
.banner text:nth-child(3){width:218rpx;height:58rpx;border:1rpx solid #fff;border-radius:10rpx;line-height:58rpx;text-align:center;margin-bottom:20rpx;}
.banner text:nth-child(4){font-size:20rpx;line-height: 20rpx;}
.profit{width:100%;padding: 0 26rpx;box-sizing:border-box;}
.profit-item{width:100%;height:125rpx;border-bottom:1rpx solid #e2e2e2;}
.item-left .item-txt{font-size:28rpx;color:#333;line-height:28rpx;}
.item-left .time{font-size:20rpx;line-height:20rpx;color:#ccc;padding-top:20rpx;}
.item-nmb{font-size:28rpx;color:#30c080;font-weight:bold;}
.empty{padding-top:160rpx;font-size:24rpx;color:#ccc;}
.empty image{margin-bottom:30rpx;width:200rpx;}
.contact{width:120rpx;position:fixed;right:20rpx;bottom:40rpx;}
.contact .iconfont{font-size:50rpx;}
.contact>view{width:120rpx;height:120rpx;font-size:24rpx;color:#fff;border-radius:50%;background:rgba(0,0,0,0.6);margin-bottom:15rpx;}

